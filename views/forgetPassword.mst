{{> header}}

<h1>Change Password</h1>

<form method="POST">
    <label>
        New password:
        <input type="password" name="password">
    </label>
    <br>
    <label>
        Confirm new password:
        <input type="password" name="password-confirm">
    </label>
    <br>
    <label>
        <input type="submit" value="Save">
    </label>
</form>
